{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": 11,
          "edge": 14,
          "firefox": 45,
          "chrome": 49,
          "safari": 10,
          "node": "8.11"
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "emotion",
    "@babel/plugin-proposal-class-properties",
    "inline-svg"
  ],
  "exclude": ["node_modules"]
}
