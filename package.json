    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "@helpscout/artboard": "^0.1.10",
    "@helpscout/helix": "^0.1.0",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-links": "^4.0.9",
    "@storybook/addon-options": "^4.0.8",
    "@storybook/addons": "^4.0.9",
    "@storybook/cli": "4.0.9",
    "@storybook/react": "^4.0.9",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.3",
    "@types/lodash.get": "^4.4.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-transition-group": "^2.0.14",
    "@types/storybook__react": "^3.0.9",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^9.2.11",
    "babel-plugin-inline-svg": "0.1.0",
    "chalk": "2.4.1",
    "check-dependencies": "1.1.0",
    "coveralls": "3.0.2",
    "cpx": "^1.5.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "dotenv": "6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "5.9.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "eslint-scope": "4.0.0",
    "flow-bin": "0.87.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "hoek": "6.1.1",
    "husky": "^1.2.0",
    "istanbul-api": "2.0.6",
    "istanbul-reports": "2.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-watch-typeahead": "^0.2.0",
    "lodash": "4.17.11",
    "mkdirp": "0.5.1",
    "node-sass": "^4.10.0",
    "np": "^3.0.4",
    "nyc": "13.1.0",
    "object-assign": "4.1.1",
    "object-values": "2.0.0",
    "prettier": "1.15.2",
    "pretty-quick": "^1.8.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-frame-component": "^1.1.0",
    "react-router": "^4",
    "react-router-dom": "^4",
    "rgb-hex": "2.1.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "sass-pathfinder": "^0.0.5",
    "seed-harvester": "^0.1.5",
    "sinon": "^7.1.1",
    "slugify": "^1.3.3",
    "style-loader": "0.23.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.6",
    "webpack": "^4.26.1"
  },
  "optionalDependencies": {
    "fsevents": "2.0.1"
  },
  "nyc": {
    "exclude": [
      "config/**/*.js",
      "dist/**/*.js",
      "scripts/**/*.js",
      "stories/**/*.js"
    ],
    "cache": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
